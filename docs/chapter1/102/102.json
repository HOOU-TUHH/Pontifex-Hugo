      "edges": [
            {
                  "data": {
                        "source": "102",
                        "target": "104",
                        "label": "A bound for the sequence is also a bound for the limit by the monotonicity of limits.",
                        "color": "#b8b8b8",
                        "style": "dashed"
                  }
            },
            {
                  "data": {
                        "source": "102",
                        "target": "108",
                        "label": "The Bolzano-Weierstrass theorem guarantees the existence of accumulation points for bounded sequences.",
                        "color": "#b8b8b8",
                        "style": "dashed"
                  }
            },
            {
                  "data": {
                        "source": "102",
                        "target": "209",
                        "label": "If the sequence of nth roots of the sequence members is bounded by a number strictly smaller than one, then the corresponding series converges by the root criterion.",
                        "color": "#b8b8b8",
                        "style": "dashed"
                  }
            },
            {
                  "data": {
                        "source": "102",
                        "target": "208",
                        "label": "If the sequence of quotients of two consecutive sequence members is bounded by a number strictly smaller than one, then the corresponding series converges by the root criterion.",
                        "color": "#b8b8b8",
                        "style": "dashed"
                  }
            },
            {
                  "data": {
                        "source": "102",
                        "target": "115",
                        "label": "Cauchy sequences are bounded.",
                        "color": "#b8b8b8",
                        "style": "dashed"
                  }
            },
            {
                  "data": {
                        "source": "102",
                        "target": "114",
                        "label": "Boundedness alone does not give convergence yet, one also needs monotonicity.",
                        "color": "#b8b8b8",
                        "style": "dashed"
                  }
            },
            {
                  "data": {
                        "source": "101",
                        "target": "102",
                        "label": "Convergent sequences are bounded but not every bounded sequence is convergent.",
                        "color": "#abe8ef",
                        "style": "solid"
                  }
            },
            {
                  "data": {
                        "source": "005",
                        "target": "102",
                        "label": "Boundedness of a sequence means that the image of the sequence is a bounded set of real numbers.",
                        "color": "#abe8ef",
                        "style": "solid"
                  }
            },
            {
                  "data": {
                        "source": "100",
                        "target": "102",
                        "label": "Boundedness is a property of sequences that tells us how large the sequence values can get at most.",
                        "color": "#abe8ef",
                        "style": "solid"
                  }
            },
            {
                  "data": {
                        "source": "013",
                        "target": "102",
                        "label": "The image of a sequence, i.e. the set of all values that the sequence attains, is a bounded set.",
                        "color": "#abe8ef",
                        "style": "solid"
                  }
            }
      ]
}
